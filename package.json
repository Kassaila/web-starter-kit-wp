  },
  "homepage": "https://github.com/justcoded/web-starter-kit-wp",
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@babel/eslint-parser": "7.13.10",
    "@babel/eslint-plugin": "7.13.10",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "7.13.8",
    "@babel/plugin-proposal-private-methods": "7.13.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.13.10",
    "@babel/preset-env": "7.13.12",
    "@babel/runtime": "7.13.10",
    "autoprefixer": "10.2.5",
    "babel-loader": "8.2.2",
    "browser-sync": "2.26.14",
    "browserslist": "4.16.3",
    "cross-env": "7.0.3",
    "del": "6.0.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint": "7.22.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "gulp": "4.0.2",
    "gulp-htmlhint": "4.0.0",
    "gulp-if": "3.0.0",
    "gulp-nunjucks": "5.1.0",
    "gulp-postcss": "9.0.0",
    "gulp-rename": "2.0.0",
    "gulp-sass": "4.1.0",
    "gulp-stylelint": "13.0.0",
    "htmlhint-stylish": "1.0.3",
    "husky": "5.2.0",
    "lint-staged": "10.5.4",
    "node-notifier": "9.0.1",
    "postcss": "8.2.8",
    "postcss-import": "14.0.0",
    "postcss-sort-media-queries": "3.6.5",
    "prettier": "2.2.1",
    "sass": "1.32.8",
    "stylelint": "13.12.0",
    "stylelint-config-recommended": "4.0.0",
    "stylelint-order": "4.1.0",
    "stylelint-scss": "3.19.0",
    "webpack": "5.27.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "postinstall": "npx gulp load-env",
    "lint-staged": "lint-staged -r --allow-empty",
    "format": "npx prettier . --write --ignore-unknown",
    "dev": "npx gulp",
    "test": "npx gulp build --log-level -LLLL",
    "build": "npx cross-env NODE_ENV=production npx gulp build",
    "lint-html": "npx gulp lint-html",
    "lint-styles": "npx gulp lint-styles",
    "lint-js": "npx gulp lint-js",
    "fix-js": "npx prettier js/**/*.js --write && npx eslint js/**/*.js --fix",
    "fix-styles": "npx prettier scss/**/*.scss --write && npx stylelint scss/**/*.scss --fix --cache"
  },
  "dependencies": {
    "normalize.css": "8.0.1"
  }
}
